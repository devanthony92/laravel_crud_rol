# =======================================
# Laravel / PHP .gitignore
# =======================================

# Dependencias de Composer
/vendor/

# Archivos de entorno y configuración local
.env
.env.backup
.env.local
.env.development
.env.production
.env.testing

# Claves de Laravel y caché
/storage/*.key
/storage/framework/cache/*
/storage/framework/sessions/*
/storage/framework/views/*
/storage/logs/*

# Archivos generados automáticamente
/bootstrap/cache/*.php

# Dependencias de Node
/node_modules/

# Archivos de build y compilación front-end
/public/hot
/public/storage
/public/mix-manifest.json
/public/js/*
/public/css/*
/public/build
/public/vendor

# IDEs y editores
.idea/
.vscode/
*.sublime-project
*.sublime-workspace

# Sistemas operativos
.DS_Store
Thumbs.db
ehthumbs.db
Icon?
desktop.ini

# Archivos temporales
*.log
*.cache
*.tmp
*.swp
*.bak
*.old

# Archivos de PHPUnit y testing
.phpunit.result.cache
.phpunit.cache/
coverage/

# Archivos locales de Git
.gitconfig
.gitignore.local

#Archivos generados por laravel
/.phpunit.cache
/node_modules
/public/build
/public/hot
/public/storage
/storage/*.key
/vendor
.env
.env.backup
.env.production
.phpunit.result.cache
Homestead.json
Homestead.yaml
auth.json
npm-debug.log
yarn-error.log
/.fleet
/.idea
/.vscode
